{
  "name": "tidb-prisma-vercel-demo",
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "next dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "setup": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/setup.mjs",
    "start": "next start",
    "vercel-build": "export DATABASE_URL=$(node ./scripts/env.mjs) && yarn run prisma:deploy && yarn run setup && yarn run build"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@faker-js/faker": "^7.6.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.93",
    "@mui/material": "^5.9.2",
    "@prisma/client": "^4.5.0",
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "next": "12.2.2",
    "notistack": "^2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.5"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "prisma": "^4.5.0",
    "typescript": "4.7.4"
  }
}
