{
  "name": "tidb-prisma-vercel-demo",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "next dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "setup": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/setup.mjs",
    "start": "next start",
    "vercel-build": "export DATABASE_URL=$(node ./scripts/env.mjs) && yarn run prisma:deploy && yarn run setup && yarn run build"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@heroicons/react": "^2.0.18",
    "@mui/lab": "^5.0.0-alpha.93",
    "@prisma/client": "^4.5.0",
    "@tailwindcss/typography": "^0.5.9",
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "next": "^13.4.13",
    "notistack": "^3.0.1",
    "postcss": "^8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.5"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.5.1",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.3.3",
    "typescript": "4.7.4"
  }
}
